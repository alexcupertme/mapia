{
  "name": "mapia",
  "version": "1.1.0",
  "description": "Type-first superfast data-mapper (JSON transformation library) for TypeScript. Fast, safe, and easy to use. Supports classes, interfaces, types",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build": "rimraf dist && tsup",
    "run:bench": "ts-node benchmark/run.ts",
    "run:shape": "ts-node src/shapes.ts",
    "run:example": "ts-node ./examples/index.ts",
    "test": "jest --coverage --detectOpenHandles --forceExit --maxWorkers=2",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexcupertme/mapia"
  },
  "author": {
    "name": "Alexander Cupertino",
    "email": "alexcupertme@gmail.com",
    "url": "https://t.me/alexcupertme"
  },
  "bugs": {
    "url": "https://github.com/samchon/typia/issues"
  },
  "license": "MIT",
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "files": [
    "dist",
    "README.md"
  ],
  "devDependencies": {
    "@automapper/classes": "^8.8.1",
    "@automapper/core": "^8.8.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.2",
    "@semantic-release/npm": "^13.1.2",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@tsconfig/node-lts": "^22.0.1",
    "@types/benchmark": "^2.1.5",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.3",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "benchmark": "^2.1.4",
    "class-transformer": "^0.5.1",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-import-x": "^4.11.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.3.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^59.0.0",
    "jest": "^29.7.0",
    "reflect-metadata": "^0.1.14",
    "rimraf": "^6.0.1",
    "semantic-release": "^25.0.2",
    "ts-jest": "^29.3.2",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.4.0",
    "typescript": "5.8.3"
  },
  "keywords": [
    "fast",
    "json",
    "mapper",
    "datamapper",
    "automapper",
    "automapper-ts",
    "stringify",
    "parse",
    "object",
    "serialization",
    "deserialization",
    "serializer",
    "transform",
    "convert",
    "typescript",
    "ajv",
    "io-ts",
    "zod",
    "typia",
    "schema",
    "json-schema",
    "generator",
    "clone",
    "runtime",
    "type",
    "typebox",
    "checker",
    "validator",
    "prune"
  ],
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json",
            "pnpm-lock.yaml"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
